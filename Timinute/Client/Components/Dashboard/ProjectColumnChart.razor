<h1>Radzen Blazor Chart column series</h1>
<div class="container">
    <div class="row">
        <div class="col-sm-12 my-5">
            <RadzenChart>
                <RadzenColumnSeries Data="@projectTimeMonth1" CategoryProperty="Project" Title="Month1" ValueProperty="TimeInt"  LineType="LineType.Dashed" />
                <RadzenColumnSeries Data="@projectTimeMonth2" CategoryProperty="Project" Title="Month2" ValueProperty="TimeInt" />
                <RadzenColumnSeries Data="@projectTimeMonth3" CategoryProperty="Project" Title="Month3" ValueProperty="TimeInt" />
                <RadzenColumnOptions Radius="5" />
                <RadzenValueAxis Formatter="@FormatAsTime">
                    <RadzenGridLines Visible="true" />
                    <RadzenAxisTitle Text="Time" />
                </RadzenValueAxis>
            </RadzenChart>
        </div>
    </div>
</div>